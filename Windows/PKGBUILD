pkgname=ure
pkgver=1.0
pkgrel=1
epoch=
pkgdesc="Universal Runtime Environment"
arch=('i686' 'x86_64' 'arm')
url="https://github.com/AnetoEnterprise/URE"
groups=('base-devel')
makedepends=('ed')
source=("$pkgname-$pkgver.tar.xz")
sha512sums=("SKIP")
package() {
mkdir -m 750 -p -v /usr/share/ure
cd $srcdir/Windows
cp -r ./ure /usr/bin/
cp -r ./lang.conf /usr/share/ure/
cp -r ./libure.dll /usr/lib/
cp -r ./libure.a /usr/lib/
}
